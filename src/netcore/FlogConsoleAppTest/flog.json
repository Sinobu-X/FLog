{
  "FLog": {
    "async": true,
    "formatterString": "%date [%thread] %level %logger - %message%newline",
    "handlers": [
      {
        "type": "Console",
        "level": "Warning",
        "includes": [
          "*"
        ],
        "excludes": [
        ]
      },
      {
        "type": "Console",
        "level": "Info",
        "maxLevel": "Info",
        "includes": [
          "*"
        ],
        "excludes": [
        ]
      },
      {
        "type": "File",
        "level": "Error",
        "includes": [
          "*"
        ],
        "excludes": [
          "FlogConsoleAppTest.DBTest"
        ],
        "folder": "logs",
        "fileName": "[yyyyMMdd].main.log"
      },
      {
        "type": "File",
        "level": "Info",
        "formatterString": "%date - %message%newline",
        "includes": [
          "FlogConsoleAppTest.DBTest"
        ],
        "excludes": [
        ],
        "folder": "logs",
        "fileName": "[yyyyMMdd].db.log"
      }
    ]
  }
}